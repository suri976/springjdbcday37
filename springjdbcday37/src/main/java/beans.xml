
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
			
		
		<bean name="garden" class="com.jobiak.ioc.Garden">
		<property name="fruits">
		<list>
		<value>Mango</value>
		<value>apple</value>
		<value>Orange</value>
		<value>Banana</value>
		
		</list>
		</property >
		
		
		<property name="flowers">
		<set>
		<value>Mago</value>
		<value>pple</value>
		<value>Onge</value>
		<value>Bnana</value>
		</set>
		</property>
		
		
		
		<property name="trees">
		<set>
		<value>Mango</value>
		<value>apple</value>
		<value>Orange</value>
		<value>Banana</value>
		</set>
		</property>
		
		<property name="cityfruit">
		<map>
		
		<entry key ="Hyderabad" value="CustardApple"></entry>
		<entry key ="Araku" value="Strawberry"></entry>
		<entry key ="Ravulapalem" value="Banana"></entry>
		<entry key ="Narayanpur" value="grapes"></entry>
		<entry key ="Anathpur" value="muskmelon"></entry>
		</map>
		</property>
		</bean>
			</beans>